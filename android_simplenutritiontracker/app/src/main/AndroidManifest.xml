<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simplenutritiontracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EditFoodGroupsActivity"></activity>
        <activity android:name=".FoodGroupDetailsActivity"></activity>
        <activity android:name=".FoodGroupListActivity"></activity>
        <activity android:name=".MacrosDetailsActivity" />
        <activity android:name=".MacrosListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MealDetailsActivity"></activity>
        <activity android:name=".MealListActivity"></activity>
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>